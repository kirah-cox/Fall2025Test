@page "/experiment"
@inject IJSRuntime JSRuntime
@using Microsoft.Extensions.DependencyInjection
@using Microsoft.JSInterop


@code {

    class SpaceShip
    {
        public double Velocity;
        public int PositionX;
        public int PositionY;
        public int RotationAngle;
        public int Lives;

        public SpaceShip()
        {
            Velocity = 0;
            PositionX = 200;
            PositionY = 250;
            RotationAngle = 0;
            Lives = 3;
        }

        public void Move()
        {
            if (WPressed && Velocity < 4)
                Velocity += 0.25;

            if (SPressed && Velocity > 0)
                Velocity -= 0.25;

            if (APressed)
                RotationAngle += 2;

            if (DPressed)
                RotationAngle -= 2;
        }
    }

    class Bullet
    {
        public int PositionX;
        public int PositionY;
        public int RotationAngle;
        public double Velocity;
        public Bullet(int posX, int posY, int rotationAngle, int velocity = 0)
        {
            PositionX = posX;
            PositionY = posY;
            RotationAngle = rotationAngle;
            Velocity = velocity;
        }

        public void Move()
        {
            PositionY -= (int)Math.Round(Math.Sin((RotationAngle * 2 * Math.PI) / 360) * Velocity);
            PositionX += (int)Math.Round(Math.Cos((RotationAngle * 2 * Math.PI) / 360) * Velocity);
        }
    }

    class Asteroid
    {
        public double PositionX;
        public double PositionY;
        public int RotationAngle;
        public double Velocity;
        public int Width;
        public int Height;
        public Asteroid(int posX, int posY, int width, int height, double velocity, int rotationAngle)
        {
            PositionX = posX;
            PositionY = posY;
            Width = width;
            Height = height;
            Velocity = velocity;
            RotationAngle = rotationAngle;
        }
        public void Move()
        {
            if (PositionY - Math.Sin((RotationAngle * 2 * Math.PI) / 360) * Velocity < 150)
            {
                PositionY = 775;
            }
            else
            {
                PositionY -= Math.Sin((RotationAngle * 2 * Math.PI) / 360) * Velocity;
            }

            if (PositionY - Math.Sin((RotationAngle * 2 * Math.PI) / 360) * Velocity > 775)
            {
                PositionY = 150;
            }
            else
            {
                PositionY -= Math.Sin((RotationAngle * 2 * Math.PI) / 360) * Velocity;
            }

            if (PositionX + Math.Cos((RotationAngle * 2 * Math.PI) / 360) * Velocity < 45)
            {
                PositionX = 1875;
            }
            else
            {
                PositionX += Math.Cos((RotationAngle * 2 * Math.PI) / 360) * Velocity;
            }

            if (PositionX + Math.Cos((RotationAngle * 2 * Math.PI) / 360) * Velocity > 1875)
            {
                PositionX = 45;
            }
            else
            {
                PositionX += Math.Cos((RotationAngle * 2 * Math.PI) / 360) * Velocity;
            }

            PositionY = (int)Math.Round(PositionY);
            PositionX = (int)Math.Round(PositionX);
        }
    }

    private DotNetObjectReference<Experiment>? _dotNetRef;
    private static string keyPressed = "None";
    private SpaceShip _SpaceShip = new SpaceShip();
    List<Bullet> Bullets = new List<Bullet>();
    List<Asteroid> Asteroids = new List<Asteroid>();
    private DateTime lastBulletTime = DateTime.MinValue;
    private readonly TimeSpan bulletCooldown = TimeSpan.FromMilliseconds(300);
    List<Bullet> bulletsToRemove = new();
    List<Asteroid> asteroidsToRemove = new();
    private static bool WPressed = false;
    private static bool SPressed = false;
    private static bool APressed = false;
    private static bool DPressed = false;
    private static bool SpacePressed = false;
    bool GameOver = false;
    bool GameWin = false;
    int Score = 0;




    protected override void OnInitialized()
    {
        SpawnInitialAsteroids();
        _ = RunGameUpdate(_SpaceShip);
    }

    private void GetRandomPosition(out int posX, out int posY)
    {
        Random rand = new Random();
        posX = rand.Next(45, 1875);
        posY = rand.Next(125, 775);
    }

    private void GetRandomRotation(out int rotationAngle)
    {
        Random rand = new Random();
        rotationAngle = rand.Next(0, 360);
    }

    private void GetRandomSize(out int size)
    {
        Random rand = new Random();
        size = rand.Next(1, 4);
    }

    private void SpawnInitialAsteroids()
    {
        int width = 0;
        int height = 0;
        for (int i = 0; i < 15; i++)
        {
            GetRandomPosition(out int posX, out int posY);
            GetRandomRotation(out int rotationAngle);
            GetRandomSize(out int size);
            if (size == 1)
            {
                width = 40;
                height = 40;
            }
            else if (size == 2)
            {
                width = 60;
                height = 60;
            }
            else
            {
                width = 80;
                height = 80;
            }

            Asteroids.Add(new Asteroid(posX, posY, width, height, 0.5, rotationAngle));
        }
    }

    private async Task RunGameUpdate(SpaceShip spaceShip)
    {
        while (true)
        {
            spaceShip.Move();

            if (spaceShip.PositionY - (int)Math.Round(Math.Sin((spaceShip.RotationAngle * 2 * Math.PI) / 360) * spaceShip.Velocity) < 150)
            {
                spaceShip.PositionY = 775;
            }
            else
            {
                spaceShip.PositionY -= (int)Math.Round(Math.Sin((spaceShip.RotationAngle * 2 * Math.PI) / 360) * spaceShip.Velocity);
            }

            if (spaceShip.PositionY - (int)Math.Round(Math.Sin((spaceShip.RotationAngle * 2 * Math.PI) / 360) * spaceShip.Velocity) > 775)
            {
                spaceShip.PositionY = 150;
            }
            else
            {
                spaceShip.PositionY -= (int)Math.Round(Math.Sin((spaceShip.RotationAngle * 2 * Math.PI) / 360) * spaceShip.Velocity);
            }

            if (spaceShip.PositionX + (int)Math.Round(Math.Cos((spaceShip.RotationAngle * 2 * Math.PI) / 360) * spaceShip.Velocity) < 45)
            {
                spaceShip.PositionX = 1875;
            }
            else
            {
                spaceShip.PositionX += (int)Math.Round(Math.Cos((spaceShip.RotationAngle * 2 * Math.PI) / 360) * spaceShip.Velocity);
            }

            if (spaceShip.PositionX + (int)Math.Round(Math.Cos((spaceShip.RotationAngle * 2 * Math.PI) / 360) * spaceShip.Velocity) > 1875)
            {
                spaceShip.PositionX = 45;
            }
            else
            {
                spaceShip.PositionX += (int)Math.Round(Math.Cos((spaceShip.RotationAngle * 2 * Math.PI) / 360) * spaceShip.Velocity);
            }

            if (!WPressed && !SPressed && spaceShip.Velocity > 0)
                spaceShip.Velocity -= 0.05;

            if (SpacePressed && (DateTime.Now - lastBulletTime) >= bulletCooldown)
            {
                Bullets.Add(new Bullet(spaceShip.PositionX + (int)(Math.Cos((spaceShip.RotationAngle * 2 * Math.PI) / 360) * 18),
                spaceShip.PositionY - (int)(Math.Sin((spaceShip.RotationAngle * 2 * Math.PI) / 360) * 18),
                spaceShip.RotationAngle, 15));

                lastBulletTime = DateTime.Now;
            }

            foreach (var bullet in Bullets)
            {
                bullet.Move();
                if (bullet.PositionX < 20 || bullet.PositionX > 1900 || bullet.PositionY < 125 || bullet.PositionY > 800)
                    bulletsToRemove.Add(bullet);

                foreach (var asteroid in Asteroids)
                {
                    double distX = bullet.PositionX - asteroid.PositionX;
                    double distY = bullet.PositionY - asteroid.PositionY;
                    double distance = Math.Sqrt(distX * distX + distY * distY);

                    if (distance < (asteroid.Width / 2))
                    {
                        bulletsToRemove.Add(bullet);
                        asteroidsToRemove.Add(asteroid);
                        if (asteroid.Width == 40)
                        {
                            Score += 100;
                        }
                        else if (asteroid.Width == 60)
                        {
                            Score += 200;
                        }
                        else
                        {
                            Score += 300;
                        }
                    }
                }
            }

            foreach (var asteroid in Asteroids)
            {
                asteroid.Move();
                double distX = spaceShip.PositionX - asteroid.PositionX;
                double distY = spaceShip.PositionY - asteroid.PositionY;
                double distance = Math.Sqrt(distX * distX + distY * distY);

                if (distance < (asteroid.Width * 3/5))
                {
                    asteroidsToRemove.Add(asteroid);
                    spaceShip.Lives -= 1;
                    spaceShip.PositionX = 200;
                    spaceShip.PositionY = 250;
                    spaceShip.RotationAngle = 0;
                }
            }

            if (spaceShip.Lives <= 0)
            {
                foreach (Asteroid asteroid in Asteroids)
                {
                    asteroidsToRemove.Add(asteroid);
                }

                GameOver = true;
            }

            if (Asteroids.Count == 0 && !GameOver)
            {
                GameWin = true;
            }

            foreach (var bullet in bulletsToRemove)
                Bullets.Remove(bullet);

            foreach (var asteroid in asteroidsToRemove)
                Asteroids.Remove(asteroid);

            await Task.Delay(16);
            await InvokeAsync(StateHasChanged);
        }
    }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            _dotNetRef = DotNetObjectReference.Create(this);
            await JSRuntime.InvokeVoidAsync("keyHandler.initialize", _dotNetRef);
        }
    }

    [JSInvokable]
    public async Task OnKeyDown(string key)
    {
        keyPressed = key.ToUpper();
        if (keyPressed == "W")
        {
            WPressed = true;
        }
        else if (keyPressed == "S")
        {
            SPressed = true;
        }
        else if (keyPressed == "A")
        {
            APressed = true;
        }
        else if (keyPressed == "D")
        {
            DPressed = true;
        }
        else if (keyPressed == " ")
        {
            SpacePressed = true;
        }
        await InvokeAsync(StateHasChanged);
    }

    [JSInvokable]
    public void OnKeyUp(string key)
    {
        keyPressed = "None";
        if (key.ToUpper() == "W")
        {
            WPressed = false;
        }
        else if (key.ToUpper() == "S")
        {
            SPressed = false;
        }
        else if (key.ToUpper() == "A")
        {
            APressed = false;
        }
        else if (key.ToUpper() == "D")
        {
            DPressed = false;
        }
        else if (key == " ")
        {
            SpacePressed = false;
        }
    }

    public void DisposeAsync()
    {
        _dotNetRef?.Dispose();
    }
}

